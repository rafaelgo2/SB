call _initBS;

store

storei 

_initBS:
	loadc R0 IO;
	loadc R1 sp; //L
	loadc R2 252; //R
	loadc R7 2;
	_while: move R3 R2;
	subtract R3 R1;
	jmpz R3 _endBS;
	loadi R4 R1; //v[L]
	loadi R5 R2; //v[R];
	move R6 R4;
	add R6 R5;
	divide R6 2; //(v[L] + v[R]) / 2
	subtract R6 R0;
	jmpp R6 _ifN;
		add R2 R1;
		divide R2 R7;
		jump _endIf;
		_ifN:
		add R1 R2;
		divide R1 R7;
		jump _endIf;
	_endIf: jump _while;
_endBS: subtract R3 
ret;


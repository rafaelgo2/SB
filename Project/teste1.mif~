DEPTH = 256 ;
WIDTH = 8 ;
ADDRESS_RADIX = HEX ;
DATA_RADIX = BIN ;
CONTENT
BEGIN

00 : 00001000 ; -- loadi R0 IO
01 : 11111110 ; -- 
02 : 01110001 ; -- clear R1
03 : 00000000 ; -- 
04 : 01101010 ; -- _new: loadc R2 1
05 : 00000001 ; -- 
06 : 00001011 ; -- loadi R3 IO
07 : 11111110 ; -- 
08 : 00101010 ; -- _loop: multiply R2 R3
09 : 01100000 ; -- 
0A : 10111011 ; -- addi R3-1
0B : 11111111 ; -- 
0C : 11010011 ; -- jmpp R3_loop
0D : 00001000 ; -- 
0E : 00010010 ; -- storei R2 IO
0F : 11111110 ; -- 
10 : 10111001 ; -- addi R1 1
11 : 00000001 ; -- 
12 : 01110010 ; -- clear R2
13 : 00000000 ; -- 
14 : 11001010 ; -- seq R2 R0 R1
15 : 00000100 ; -- 
16 : 01000010 ; -- jmpz R2_new
17 : 00000100 ; -- 
[18..FF] : 00000000 ; -- 
END ;
